<Window x:Class="kuvankasittely.ColorPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:kuvankasittely"
        mc:Ignorable="d"
        Title="Paletti" Height="307" Width="244" ResizeMode="NoResize">
    <Grid Margin="10">
        <xctk:ColorCanvas x:Name="ColorCanvas"
                          SelectedColorChanged="ColorCanvas_SelectedColorChanged" Margin="-10,-10,-10,-10" />
    </Grid>
</Window>
